<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.paquetcoachapp"
    android:versionCode="1"
    android:versionName="1.0" >
<uses-sdk 
    android:minSdkVersion="14"
          android:targetSdkVersion="19"/>


    <uses-permission android:name="android.permission.BLUETOOTH_PRIVILEGED" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name="com.example.paquetcoachapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.paquetcoachapp.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.example.paquetcoachapp.StatsActivity"
            android:label="@string/title_activity_stats" >
        </activity>
        <activity
            android:name="com.example.paquetcoachapp.ReseauActivity"
            android:label="@string/title_activity_reseau" >
        </activity>
        <activity
            android:name="com.example.paquetcoachapp.FakePackActivity"
            android:label="@string/title_activity_fake_pack" >
        </activity>
        <activity
            android:name="com.example.paquetcoachapp.FakeDate"
            android:label="@string/title_activity_fake_date" >
        </activity>
        <activity
            android:name="com.example.paquetcoachapp.BluetoothTestActivity"
            android:label="@string/title_activity_bleutooth_test" >
        </activity>
        <activity
            android:name="com.example.paquetcoachapp.StatFormat"
            android:label="@string/title_activity_stat_format" >
        </activity>
        <activity
            android:name="com.example.paquetcoachapp.FiguresActivity"
            android:label="@string/title_activity_figures" >
        </activity>
        <activity
            android:name="com.example.paquetcoachapp.ParametresActivity"
            android:label="@string/title_activity_parametres" >
        </activity>
        <activity
            android:name="com.example.paquetcoachapp.ParametresReseauActivity"
            android:label="@string/title_activity_reseau_parametres" >
        </activity>
    </application>

</manifest>
